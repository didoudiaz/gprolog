#!/bin/sh
do_diff() {
    diff --ignore-space-change -I '% date' -I '% compiler' -I '% time' $1 $2
}

pl_files='pl2wam read_file bip_list context syn_sugar internal code_gen reg_alloc inst_codif first_arg indexing wam_emit'


files=${*:-$pl_files}
for i in $files; do
	do_diff $i.wam $i.wam1
    done


